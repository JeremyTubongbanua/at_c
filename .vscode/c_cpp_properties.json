{
    "configurations": [
        {
            "name": "default",
            "includePath": [
                "${default}",
                "${workspaceFolder}/include/**",
                "${workspaceFolder}/deps/mbedtls/include/**"
            ],
            "compilerPath": "/usr/bin/gcc",
            "cStandard": "c99",
            "cppStandard": "c++17",
            "defines": [
                "BUILD_MBEDTLS=1",
                "BUILD_ARDUINO",
                "BUILD_ESP_IDF"
            ],
            "configurationProvider": "ms-vscode.cmake-tools"
        }
    ],
    "version": 4
}